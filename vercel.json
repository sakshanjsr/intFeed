{
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/users.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/users"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "installCommand": "npm install --prefix=frontend"
}