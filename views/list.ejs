<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>intFeed - Records</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <header>
        <h1>All Stored Records</h1>
    </header>
    <main>
        <% if (users && users.length > 0) { %>
            <table>
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Age</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user => { %>
                        <tr>
                            <td><%= user.id %></td>
                            <td><%= user.name %></td>
                            <td><%= user.age %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        <% } else { %>
            <p>No records found.</p>
        <% } %>
        <p><a href="/">Go back to home</a></p>
    </main>
</body>
</html>